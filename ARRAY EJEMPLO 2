# include <iostream>
using namespace std;
int main () {
// Crear un arreglo unidimensional de tama~no 5
int numeros [5];
// Asignar valores mediante una iteraciÂ´on
for (int i = 0; i < 5; i ++) {
numeros [i] = i * 2;
 }
// Mostrar los valores del arreglo
for (int i = 0; i < 5; i ++) {
cout << " numeros [" << i << "] = " << numeros [i] <<endl ;
}
return 0;
}
